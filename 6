import java.util.Scanner;

public class DataTypesFitting {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int t = Integer.parseInt(scanner.nextLine()); // Read number of test cases
        
        while (t-- > 0) {
            String input = scanner.nextLine();
            try {
                long number = Long.parseLong(input); // Try to parse it as long
                
                System.out.println(number + " can be fitted in:");
                
                // Check which data types can fit the number
                if (number >= Byte.MIN_VALUE && number <= Byte.MAX_VALUE) {
                    System.out.println("* byte");
                }
                if (number >= Short.MIN_VALUE && number <= Short.MAX_VALUE) {
                    System.out.println("* short");
                }
                if (number >= Integer.MIN_VALUE && number <= Integer.MAX_VALUE) {
                    System.out.println("* int");
                }
                // If it fits in long, we will show it as well
                System.out.println("* long");
            } catch (NumberFormatException e) {
                System.out.println(input + " can't be fitted anywhere.");
            }
        }
        
        scanner.close();
    }
}
